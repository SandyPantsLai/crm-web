<h1>Create a new contact</h1>

<form action="/contacts" method="post">
  <p>First name: </p>
  <input type="text" name="first_name">
  <p>Last name: </p>
  <input type="text" name="last_name">
  <p>Email: </p>
  <input type="text" name="email">
  <p>Notes: </p>
  <input type="text" name="note">
  <br/>
  <input type="submit" name="Submit">
</form>